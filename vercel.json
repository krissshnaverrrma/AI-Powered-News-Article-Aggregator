{
  "version": 2,
  "builds": [
    {
      "src": "AI-Powered-News--Article-Aggregator/core/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "AI-Powered-News--Article-Aggregator/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "AI-Powered-News--Article-Aggregator/core/wsgi.py"
    }
  ]
}
